    <xs:complexType name="BVG-LPP-ContributionType">
        <xs:sequence>
            <xs:element name="BVG-LPP-Code" type="tns:BVG-LPP-AssuranceCategoryCodeDiffType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>BVG-Code</translation>
                        <description>
                            <short>BVG-Code</short>
                            <domain>
                                Mit dem BVG-Code wird angegeben, welcher Versicherungskategorie
                                der Mitarbeiter zugeteilt wurde. Er weist auf die Art der Versicherungslösung hin.
                            </domain>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Code LPP</translation>
                        <description>
                            <short>Code LPP</short>
                            <domain>
                                Le code LPP indique la catégorie d'assurance de l'assuré. Il désigne
                                le genre de la solution d'assurance.
                            </domain>
                        </description>
                    </xs:documentation>
                    <xs:documentation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:choice minOccurs="0">
                <xs:sequence>
                    <xs:element name="ValidAsOf" type="xs:date" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>gültig ab</translation>
                                <description>
                                    <short>gültig ab pro Person</short>
                                    <domain>
                                        Im Normalfall wird das 'generell gültig ab' verwendet.
                                        In Ausnahmen kann mit diesem 'gültig ab' ein Datum pro Person gesetzt werden.
                                    </domain>
                                </description>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>valide à partir de</translation>
                                <description>
                                    <short>valide à partir de selon personne</short>
                                    <domain>
                                        Normalement une valeur générelle est utilisée. Dans des cas exceptionnels une
                                        date par personne peut être émise.
                                    </domain>
                                </description>
                            </xs:documentation>
                            <xs:documentation>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="EmployeeContribution" type="tns:SalaryAmountType">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>Arbeitnehmerbeitrag</translation>
                                <description>
                                    <short>Kostenbeitrag einer versicherten Person, den sie über die Lohnabzüge
                                        entrichtet.
                                    </short>
                                </description>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>Contribution d'employé</translation>
                                <description>
                                    <short>Contribution d'une personne assurée selon les retenues de salaire</short>
                                </description>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="EmployerContribution" type="tns:SalaryAmountType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>Arbeitgeberbeitrag</translation>
                                <description>
                                    <short>Teil der Kosten, die dem Arbeitgeber erwachsen.</short>
                                </description>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>contributions d'employeur</translation>
                                <description>
                                    <short>Coûts pour l'employeur</short>
                                </description>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ThirdPartyContribution" type="tns:SalaryAmountType"
                                minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>Beiträge von Dritten</translation>
                                <description>
                                    <short>BVG-Beiträge von einem weiteren Beitragszahler
                                        (z.B. Fonds der Vorsorgeeinrichtung)
                                    </short>
                                </description>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>Contributions de tiers</translation>
                                <description>
                                    <short>Contributions LPP d'autres contributeurs (fonds de prévoyance)</short>
                                </description>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:choice>
                    <xs:element name="Unknown" type="tns:EmptyType">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>unbekannt</translation>
                                <description>
                                    <short>
                                        Der übermittelte BVG-Code ist beim Versicherer nicht bekannt.
                                    </short>
                                    <domain>
                                        TODO
                                    </domain>
                                </description>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>inconnu</translation>
                                <description>
                                    <short>
                                        Le code LPP n'est pas connu chez l'assureur.
                                    </short>
                                    <domain>
                                        TODO
                                    </domain>
                                </description>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Missing" type="tns:EmptyType">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>fehlend</translation>
                                <description>
                                    <short>
                                        Der BVG-Code wurde nicht übermittelt und fehlt deshalb.
                                    </short>
                                    <domain>
                                        TODO
                                    </domain>
                                </description>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>manquant</translation>
                                <description>
                                    <short>
                                        Le code LPP manque car il n'a pas été transmis.
                                    </short>
                                    <domain>
                                        TODO
                                    </domain>
                                </description>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="NoContributionsAvailable" type="tns:EmptyType">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>Beitrag nicht verfügbar</translation>
                                <description>
                                    <short>
                                        Der BVG-Beitrag ist nicht verfügbar, da er aus unbekannten Gründen nicht
                                        berechnet werden konnte.
                                    </short>
                                    <domain>
                                        TODO
                                    </domain>
                                </description>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>Contribution non disponible</translation>
                                <description>
                                    <short>
                                        La contribution LPP n'est pas disponible car elle n'a pas pu être calculée pour
                                        des causes inconnues.
                                    </short>
                                    <domain>
                                        TODO
                                    </domain>
                                </description>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:choice>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>
