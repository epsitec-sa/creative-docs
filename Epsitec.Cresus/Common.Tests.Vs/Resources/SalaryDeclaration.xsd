    <xs:complexType name="FAK-CAF-SalaryType">
        <xs:sequence>
            <xs:element name="FAK-CAF-Period" type="tns:TimePeriodType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>FAK-Periode</translation>
                        <description>
                            <short>
                                FAK-Periode
                            </short>
