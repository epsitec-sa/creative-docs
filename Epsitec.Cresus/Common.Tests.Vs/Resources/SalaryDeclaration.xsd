                        <description>
                            <short>Geburtsdatum des Kindes</short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Date de naissance</translation>
                        <description>
                            <short>Date de naissance de l'enfant</short>
                        </description>
                    </xs:documentation>
                    <xs:documentation>
                        <usage>FAK</usage>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SV-AS-Number" type="tns:SV-AS-NumberType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Sozialversicherungsnummer</translation>
                        <description>
                            <short>Von der Zentrale-Ausgleichsstelle (ZAS) zugeteilte Sozialversicherungsnummer</short>
                            <domain>Ab Einführung ist die 13-stellige Sozialversicherungsnummer zu erfassen.
                                Diese wird von der Zentralen Ausgleichsstelle zugeteilt.
                            </domain>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Numéro d'assurance sociale</translation>
                        <description>
                            <short>Numéro d'assurance sociale attribué par la centrale de compensation (CdC).</short>
                            <domain>À partir de l'initiation du numéro d'assurance sociale à 13 chiffres, il faut
                                l'indiquer. Le numéro est attribué par la centrale de compensation (CdC).
                            </domain>
                        </description>
