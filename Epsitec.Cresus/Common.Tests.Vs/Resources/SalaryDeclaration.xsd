    <!-- **PatchInfo V3: Fachgruppe Statistik          -->
    <xs:simpleType name="EmploymentContractType">
        <xs:annotation>
            <xs:documentation xml:lang="de">
                <translation>Arbeitsvertragsart</translation>
                <description>
                    <short>Arbeitsvertragsart angeben</short>
                    <domain>
                        Mögliche Werte:
                        indefiniteSalaryMth = unbefristeter Vertrag mit Monatslohn
                        (Indefinite duration with salary per month)
                        indefiniteSalaryMthAWT = unbefristeter Vertrag mit Monatslohn und Jahresarbeitszeit
                        (Indefinite duration with salary per month and annual working time model)
                        indefiniteSalaryHrs = unbefristeter Vertrag mit Stundenlohn
                        (Indefinite duration with salary per month)
                        fixedSalaryMth = befristeter Vertrag mit Monatslohn
                        (Temporary/fixed* duration salary per month)
                        fixedSalaryHrs = befristeter Vertrag mit Stundenlohn
                        (Temporary/fixed* duration salary per hour)
                        (* Unter die befristeten Verträge fallen Praktikanten und Studenten mit einem Lohn,
                        hingegen die Lehrlinge nicht.)
                        apprentice = Lehrvertrag
                    </domain>
                </description>
            </xs:documentation>
            <xs:documentation xml:lang="fr">
                <translation>Type de contrat de travail</translation>
                <description>
                    <short>Indiquer le type de contrat de travail</short>
                    <domain>
                        Valeurs possibles:
                        indefiniteSalaryMth = contrat illimité, salaire mensuel
                        (Indefinite duration with salary per month)
                        indefiniteSalaryMthAWT = contrat illimité, salaire mensuel, temps de travail annuel
                        (Indefinite duration with salary per month and annual working time model)
                        indefiniteSalaryHrs = contrat illimité, salaire horaire
                        (Indefinite duration with salary per month)
                        fixedSalaryMth = contrat limité, salaire mensuel
                        (Temporary/fixed* duration salary per month)
                        fixedSalaryHrs = contrat limité, salaire horaire
                        (Temporary/fixed* duration salary per hour)
                        (* les contrats limités inclurent des pratiquants/étudiants, mais pas les apprentis)
                        apprentice = contrat d'apprenti
                    </domain>
                </description>
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="indefiniteSalaryMth"/>
            <xs:enumeration value="indefiniteSalaryMthAWT"/>
            <xs:enumeration value="indefiniteSalaryHrs"/>
            <xs:enumeration value="fixedSalaryMth"/>
            <xs:enumeration value="fixedSalaryHrs"/>
            <xs:enumeration value="apprentice"/>
        </xs:restriction>
    </xs:simpleType>
