    <xs:complexType name="GeneralSalaryDeclarationDescriptionType">
        <xs:sequence>
            <xs:element name="CreationDate" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Erstellungsdatum</translation>
                        <description>
                            <short>
                                Datum und Zeit der Meldungserstellung bei dem Unternehmen.
                            </short>
                            <domain>
                                Dieses Datum kann NICHT für die Berechnung von Verzugszinsen verwendet werden.
                            </domain>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Date de création</translation>
                        <description>
                            <short>
                                Date et heure de la création de la déclaration auprès de l'entreprise.
                            </short>
                            <domain>
                                Cette date ne doit PAS être utilisée pour le calcul d'intérêts moratoires.
                            </domain>
