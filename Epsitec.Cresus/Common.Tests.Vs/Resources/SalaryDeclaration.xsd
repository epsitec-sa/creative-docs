                    <xs:field xpath="@institutionID"/>
                </xs:key>
                <xs:keyref name="UVGZ-LAAC-ID-Ref" refer="tns:UVGZ-LAAC-ID-Key">
                    <xs:selector
                            xpath="tns:Staff/tns:Person/tns:UVGZ-LAAC-Salaries/tns:UVGZ-LAAC-Salary|tns:SalaryTotals/tns:UVGZ-LAAC-Totals"/>
                    <xs:field xpath="@institutionIDRef"/>
                </xs:keyref>
                <xs:key name="KTG-AMC-ID-Key">
                    <xs:selector xpath="tns:Insurances/tns:KTG-AMC"/>
                    <xs:field xpath="@institutionID"/>
                </xs:key>
                <xs:keyref name="KTG-AMC-ID-Ref" refer="tns:KTG-AMC-ID-Key">
                    <xs:selector
                            xpath="tns:Staff/tns:Person/tns:KTG-AMC-Salaries/tns:KTG-AMC-Salary|tns:SalaryTotals/tns:KTG-AMC-Totals"/>
                    <xs:field xpath="@institutionIDRef"/>
                </xs:keyref>
                <xs:key name="BVG-LPP-ID-Key">
                    <xs:selector xpath="tns:Insurances/tns:BVG-LPP"/>
                    <xs:field xpath="@institutionID"/>
                </xs:key>
                <xs:keyref name="BVG-LPP-ID-Ref" refer="tns:BVG-LPP-ID-Key">
                    <xs:selector
                            xpath="tns:Staff/tns:Person/tns:BVG-LPP-Salaries/tns:BVG-LPP-Salary"/>
                    <xs:field xpath="@institutionIDRef"/>
                </xs:keyref>
                <!-- **PatchInfo V3: Fachgruppe AHV/FAK            -->
                <xs:key name="FAK-CAF-ID-Key">
                    <xs:selector xpath="tns:Insurances/tns:FAK-CAF"/>
                    <xs:field xpath="@institutionID"/>
                </xs:key>
                <xs:keyref name="FAK-CAF-ID-Ref" refer="tns:FAK-CAF-ID-Key">
                    <xs:selector
                            xpath="tns:Staff/tns:Person/tns:FAK-CAF-Salaries/tns:FAK-CAF-Salary|tns:SalaryTotals/tns:FAK-CAF-Totals"/>
                    <xs:field xpath="@institutionIDRef"/>
                </xs:keyref>
                <xs:key name="CompanyWorkingTimeID-Key">
                    <xs:selector xpath="tns:CompanyDescription/tns:BUR-REE/tns:CompanyWorkingTime"/>
                    <xs:field xpath="@CompanyWorkingTimeID"/>
                </xs:key>
                <xs:keyref name="CompanyWorkingTimeID-Ref" refer="tns:CompanyWorkingTimeID-Key">
                    <xs:selector
                            xpath="tns:Staff/tns:Person/tns:StatisticSalaries/tns:StatisticSalary"/>
                    <xs:field xpath="@CompanyWorkingTimeIDRef"/>
                </xs:keyref>
            </xs:element>
            <xs:element name="GeneralSalaryDeclarationDescription" type="tns:GeneralSalaryDeclarationDescriptionType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Allgemeine Beschreibung der Meldung</translation>
                        <description>
                            <short>
                                Hier befinden sich Informationen, welche bei der Erstellung der Meldung durch den
                                Ersteller bzw. Absender ergänzt werden.
                            </short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Description générale de la déclaration de salaires</translation>
                        <description>
                            <short>
                                Dans cette partie ce trouvent des informations à completer par l'expéditeur.
                            </short>
                        </description>
                    </xs:documentation>
                    <xs:documentation>
                        <usage>all</usage>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="schemaVersion" type="tns:SupportedMinorSchemaVersionAttributeType" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="de">
                    <translation>Schemaversion</translation>
                    <description>
                        <short>
                            Die XML Schema Instanz muss mit dem "schemaVersion" Attribut angeben mit welcher
                            Schemaversion sie konform ist.
                        </short>
                    </description>
                </xs:documentation>
                <xs:documentation xml:lang="fr">
                    <translation>Version du schéma</translation>
                    <description>
                        <short>
                            L'attribut "schemaVersion" indique la version du schéma avec laquelle l'instance
                            est en accord.
                        </short>
                    </description>
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
