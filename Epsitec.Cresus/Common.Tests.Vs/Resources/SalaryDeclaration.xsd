                </xs:key>
                <xs:keyref name="UVG-LAA-ID-Ref" refer="tns:UVG-LAA-ID-Key">
                    <xs:selector
                            xpath="tns:Staff/tns:Person/tns:UVG-LAA-Salaries/tns:UVG-LAA-Salary|tns:SalaryTotals/tns:UVG-LAA-Totals"/>
                    <xs:field xpath="@institutionIDRef"/>
