<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>Insert a Copyright notice</Title>
            <Shortcut>CR</Shortcut>
            <Description>Code snippet for the Copyright notice comment</Description>
            <Author>Pierre Arnaud</Author>
            <SnippetTypes>
              <SnippetType>Expansion</SnippetType>
            </SnippetTypes>
        </Header>
        <Snippet>
            <Code Language="csharp">
                <![CDATA[//	Copyright © 2012, EPSITEC SA, CH-1400 Yverdon-les-Bains, Switzerland
//	Author: Pierre ARNAUD, Maintainer: Pierre ARNAUD

$end$]]>
            </Code>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>
