<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
        targetNamespace="http://www.swissdec.ch/schema/sd/20090803/SalaryDeclarationContainer"
        xmlns:tns="http://www.swissdec.ch/schema/sd/20090803/SalaryDeclarationContainer"
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns:sd="http://www.swissdec.ch/schema/sd/20090803/SalaryDeclaration"
        elementFormDefault="qualified"
        attributeFormDefault="unqualified"
        version="0.0">

    <!-- state of standardisation: FINAL
         publication: 2009-12-22
         a. boehm
    -->

    <xs:import namespace="http://www.swissdec.ch/schema/sd/20090803/SalaryDeclaration"
               schemaLocation="SalaryDeclaration.xsd"/>

    <xs:complexType name="RequestContextType">
        <xs:sequence>
            <xs:element name="UserAgent" type="tns:UserAgentType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Applikationsinformation</translation>
                        <description>
                            <short>
                                Beschreibung der wesentlichen System-Identifikations-Daten
                            </short>
                            <technical>
                                Zur Qualitätsicherung des Übermittlungsprozesses werden die wesentlichen Informationen
                                der beteiligten gesichert
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Informations concernant l'application</translation>
                        <description>
                            <short>
                                Description des données essentielles d'identification de système
                            </short>
                            <technical>
                                Pour l'assurance de la qualité, les informations essentielles des participants
                                sont sauvegardées.
                            </technical>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CompanyName" type="sd:CompanyNameType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Unternehmensdaten</translation>
                        <description>
                            <short>
                                Beschreibung der wesentlichen Unternehmensdaten
                            </short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Nom de l'entreprise</translation>
                        <description>
                            <short>
                                Nom de l'entreprise
                            </short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TransmissionDate" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Übertragungszeitpunkt</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Date et Temps de transmission</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RequestID" type="tns:IDType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>AuftragsID</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Identificateur de la demande</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="LanguageCode" type="tns:LanguageCodeType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Sprachcode</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Code de langage</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TestCase" type="tns:EmptyType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Testfall</translation>
                        <description>
                            <short>
                                Testfall-Kennzeichnung
                            </short>
                            <technical>
                                Die Daten sollen empfangen und quittiert werden.
                                Aber OHNE PRODUKTIVE VERARBEITUNG!!!
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Cas de test</translation>
                        <description>
                            <short>
                                Identificateur pour des cas de test
                            </short>
                            <technical>
                                Reception et quittance des données, mais
                                SANS TRAITEMENT PRODUCTIF!!!
                            </technical>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="MonitoringID" type="tns:IDType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>ÜberwachungsID</translation>
                        <description>
                            <short>
                                ÜberwachungsID
                            </short>
                            <technical>
                                Die ÜberwachungsID wird vorallem in der Referenzapplikation zur Aufteilung der Daten
                                verwendet
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Identificateur de surveillance</translation>
                        <description>
                            <short>
                                Identificateur de surveillance
                            </short>
                            <technical>
                                L'identificateur de surveillance est surtout utilisé dans l'application de
                                référence pour le classement des données.
                            </technical>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PartialDeclaration" type="tns:PartialDeclarationType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Teillieferung</translation>
                        <description>
                            <short>
                                Meldung bestehend aus mehrern Übermittlungen
                            </short>
                            <technical>
                                Im Moment noch nicht aktiviert!
                                XXL-ELM muss noch spezifiziert werden.
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Declaration partielle</translation>
                        <description>
                            <short>
                                Declaration composée d'éléments de plusieurs transmissions
                            </short>
                            <technical>
                                Pas encore disponible!
                                XXL-ELM n'est pas spécifié complètement
                            </technical>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SalaryDeclarationRequestType">
        <xs:sequence>
            <xs:element name="RequestContext" type="tns:RequestContextType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>AuftragsKontext</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Contexte de la demande</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Job" type="tns:JobType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Arbeitsauftrag</translation>
                        <description>
                            <short>
                                Arbeitsauftrag für sämtliche Empfänger (Distributor und Institutionen)
                            </short>
                            <technical>
                                Hier sind alle Empfänger aufgeführt
                                - Distributor
                                - Institutions-Empfänger der Domänen
                                - carbon copy Treuhänder und Broker
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Commande</translation>
                        <description>
                            <short>
                                Commande pour tous les receveurs (distributeur et institutions)
                            </short>
                            <technical>
                                Liste de tous les receveurs
                                - distributeur
                                - receveurs des institutions des domaines
                                - receveurs "carbon copy" : agents fiduciaires, courtiers
                            </technical>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Substitution" type="tns:SubstitutionType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Substitution</translation>
                        <description>
                            <short>
                                Substitution einer gültigen Meldung
                            </short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Substitution</translation>
                        <description>
                            <short>
                                Substitution d'un avis valide
                            </short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SalaryDeclaration" type="sd:SalaryDeclarationType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Lohnmeldung</translation>
                        <description>
                            <short>
                                Nutzdaten zur Lohnmeldung
                            </short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Avis de salaire</translation>
                        <description>
                            <short>
                                Données utilitaires de l'avis de salaire
                            </short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="GetStatusFromDeclareSalaryDeferredType">
        <xs:sequence>
            <xs:element name="RequestContext" type="tns:RequestContextType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>AuftragsKontext</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Contexte de la demande</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="JobKey" type="tns:JobKeyType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Auftrags-Schlüssel</translation>
                        <description>
                            <short>
                                Auftrags-Schlüssel für die zweite Operation: GetStatusFromDeferredDeclaration
                            </short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Clé de la commande</translation>
                        <description>
                            <short>
                                Clé de la commande pour pouvaoir exécuter la deuxième opération:
                                GetStatusFromDeferredDeclaration
                            </short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="JobType">
        <xs:sequence>
            <xs:element name="Addressees" type="tns:AddresseeType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Adressen</translation>
                        <description>
                            <short>
                                Adressen der Empfänger
                            </short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Adresses</translation>
                        <description>
                            <short>
                                Adresses des receveurs
                            </short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
                <xs:unique name="AHV-AVS-Ctrl">
                    <xs:selector xpath="tns:AHV-AVS"/>
                    <xs:field xpath="@institutionIDRef"/>
                </xs:unique>
                <xs:unique name="UVG-LAA-Ctrl">
                    <xs:selector xpath="tns:UVG-LAA"/>
                    <xs:field xpath="@institutionIDRef"/>
                </xs:unique>
                <xs:unique name="UVGZ-LAAC-Ctrl">
                    <xs:selector xpath="tns:UVGZ-LAAC"/>
                    <xs:field xpath="@institutionIDRef"/>
                </xs:unique>
                <xs:unique name="KTG-AMC-Ctrl">
                    <xs:selector xpath="tns:KTG-AMC"/>
                    <xs:field xpath="@institutionIDRef"/>
                </xs:unique>
                <xs:unique name="BVG-LPP-Ctrl">
                    <xs:selector xpath="tns:BVG-LPP"/>
                    <xs:field xpath="@institutionIDRef"/>
                </xs:unique>
                <xs:unique name="FAK-CAF-Ctrl">
                    <xs:selector xpath="tns:FAK-CAF"/>
                    <xs:field xpath="@institutionIDRef"/>
                </xs:unique>
            </xs:element>
            <xs:element name="EndUserNotification" type="tns:UserNotificationType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Endbenutzer-Benachrichtigung</translation>
                        <description>
                            <short>
                                Benachrichtigung des Endbenutzer mittels eMail über das Ende der Verteilung
                                (Distributor)
                            </short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Notification de l'utilisateur final</translation>
                        <description>
                            <short>
                                Notification de l'utilisateur final par e-mail, quand la distribution est terminée
                                (distributeur)
                            </short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="AddresseeType">
        <xs:sequence>
            <xs:element name="AHV-AVS" type="tns:ControlsMultiType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>AHV-UVG Steuerung</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Contrôle AHV-UVG</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="UVG-LAA" type="tns:ControlsMultiType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>UVG-LAA Steuerung</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Contrôle UVG-LAA</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="UVGZ-LAAC" type="tns:ControlsMultiType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>UVGZ-LAAC Steuerung</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Contrôle UVGZ-LAAC</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="KTG-AMC" type="tns:ControlsMultiType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>KTG-AMC Steuerung</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Contrôle KTG-AMC</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BVG-LPP" type="tns:ControlsMultiType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>BVG-LPP Steuerung</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Contrôle BVG-LPP</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FAK-CAF" type="tns:ControlsMultiType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>FAK-CAF Steuerung</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Contrôle FAK-CAF</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CantonalTaxAdministration" type="tns:ControlsType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Kt.Stv. Steuerung</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Contrôle administration fiscale cantonale</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SwissFederalStatisticalOffice" type="tns:ControlsType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>BFS Steuerung</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Contrôle OFS</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ControlsType">
        <xs:sequence>
            <xs:element name="ProcessByDistributor" type="sd:SimpleBooleanType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Verteilung durch Distributor</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Distribution effectuée par le distributeur</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CompletionUnwanted" type="sd:EmptyType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Completion nicht erwünscht</translation>
                        <description>
                            <short>
                                Ein optionales Completion ist nur beim Vorabgleich zulässig!
                                In der normalen Lohnmeldung muss der Endbenutzer immer
                                ein Completion und damit die Freigabe der Meldung machen.
                            </short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Complétion non souhaitée</translation>
                        <description>
                            <short>
                                Une completion optionelle n'est possible que dans le cas d'un collationnement.
                                Une declaration salariale normale doit toujours être libéré par l'utilisateur
                                qui doit donc toujours exécuter la completion.
                            </short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CCAddressee" type="tns:CarbonCopyAddresseeType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Adresse für Datenkopie</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Adresse receveur pour une copie des données</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ControlsMultiType">
        <xs:complexContent>
            <xs:extension base="tns:ControlsType">
                <xs:attribute name="institutionIDRef" type="sd:InstitutionIDType" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="UserNotificationType">
        <xs:annotation>
            <xs:documentation>EndBenutzer Benachrichtigung</xs:documentation>
            <xs:documentation xml:lang="fr">Notification de l'utilisateur final</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Name" type="xs:string">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Name</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Nom</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="EmailAddress" type="tns:EmailAddressType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Email Adresse</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Adresse e-mail</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PhoneNumber" type="xs:string">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Telefonnummer</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Numéro de téléphone</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SubstitutionType">
        <xs:sequence>
            <xs:element name="PredecessorRequestID" type="tns:IDType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>AuftragsID des Vorgänger</translation>
                        <description>
                            <short>
                                AuftragsID der Meldung welche ersetzt werden soll
                            </short>
                            <technical>
                                Es können nur gültige Meldungen ersetzt werden, d.h.
                                Auftrag wurde AKZEPTIERT
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Numéro de la demande du prédécesseur</translation>
                        <description>
                            <short>
                                Numéro de la demande de l'avis à remplacer
                            </short>
                            <technical>
                                Le remplacement n'est possible que pour des avis valides,
                                c.a.d. la commande a été ACCEPTÉE
                            </technical>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PredecessorResponseIDWithAcceptedState" type="tns:IDType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>AntwortID des Vorgänger</translation>
                        <description>
                            <short>
                                AntwortID der Meldung welche ersetzt werden soll
                            </short>
                            <technical>
                                Es können nur gültige Meldungen ersetzt werden, d.h.
                                Auftrag wurde AKZEPTIERT
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Identificateur de réponse du prédécesseur</translation>
                        <description>
                            <short>
                                Identificateur de l'avis à remplacer
                            </short>
                            <technical>
                                Le remplacement n'est possible que pour des avis valides,
                                c.a.d. la commande a été ACCEPTÉE
                            </technical>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CarbonCopyAddresseeType">
        <xs:sequence>
            <xs:element name="PartnerID" type="tns:IDType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Partneridentifikation</translation>
                        <description>
                            <short>Partneridentifikation gemäss seperater Liste</short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Identificateur de partenaire</translation>
                        <description>
                            <short>Identification de partenaire selon liste séparée</short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Name" type="xs:string">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Name</translation>
                        <description>
                            <short>Name (1. Zeile)</short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Nom</translation>
                        <description>
                            <short>Nom (1e ligne)</short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ComplementaryLine" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Zusatzzeile</translation>
                        <description>
                            <short>Zusatzzeile für Postadresse (2. Zeile)</short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Ligne accessoire</translation>
                        <description>
                            <short>Ligne accessoire pour adresse postale (2e ligne)</short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Street" type="xs:string">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Strasse</translation>
                        <description>
                            <short>Strasse und Hausnummer (3. Zeile)</short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Rue</translation>
                        <description>
                            <short>Rue et numéro (3e ligne)</short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Postbox" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Postfach</translation>
                        <description>
                            <short>Postfach (4. Zeile)</short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Case postale</translation>
                        <description>
                            <short>Case postale (4e ligne)</short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ZIP-Code" type="tns:ZIP-CodeType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Postleitzahl (PLZ)</translation>
                        <description>
                            <short>Postleitzahl (5. Zeile)</short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Code postal (CP)</translation>
                        <description>
                            <short>Code postal (5e ligne)</short>
                        </description>
                    </xs:documentation>
